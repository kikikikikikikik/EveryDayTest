    //数字颠倒
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        StringBuilder str=new StringBuilder();
        while(n>=10){
            str.append(n%10);
            n=n/10;
        }
        str.append(n);
        System.out.println(str.toString());
    }
}
